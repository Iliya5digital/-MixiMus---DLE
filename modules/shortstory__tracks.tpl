<article class="track__item flex flex__middle flex__between js-item" data-poster="[xfvalue_image_url_poster]" data-track="{THEME}/images/demo1.mp3" data-artist="Artist" data-title="{title}">
	[xfgiven_poster]<div class="track__item-poster"><figure><img src="[xfvalue_image_url_poster]" alt="{title}" width="55" height="55"/></figure></div>[/xfgiven_poster]
	<div class="track__item-metatitle">
		<div class="track__item-title"><a href="{full-link}" title="{title}">{title}</a></div>
		<div class="track__item-subtitle">Artist</div>
	</div>
	<div class="track__item-meta">
      <span class="dropdown-button aicon aicon-show-more-button-with-three-dots" data-activates='dropdowns'></span>
      <ul id='dropdowns' class='dropdown-content'>
        <li>
        	[not-group=5]
					<div class="track__item-favourite flex flex__middle flex__around">
						[add-favorites]
						<span>Add favorites</span>
						<span class="aicon-Heart" title="Add Favorites">
				      <span class="path1"></span><span class="path2"></span>
				    </span>
				    [/add-favorites]
				    [del-favorites]
				    <span>Del favorites</span>
						<span class="aicon-Heart" title="Delete Favorites">
				      <span class="path1"></span><span class="path2"></span>
				    </span>
				    [/del-favorites]
					</div>
					[/not-group]
        </li>
        <li>
        	<div class="track__item-download">
						<a href="{THEME}/images/demo1.mp3" download target="_blank" title="Download" class="flex flex__middle flex__around">
							<span>Download</span>
							<span class="aicon-Download">
					      <span class="path1"></span><span class="path2"></span><span class="path3"></span>
					    </span>
						</a>
					</div>
        </li>
      </ul>
  </div>
	<div class="track__item-play js-ctrl js-play" title="Play/Stop">
		<span class="aicon aicon-play"></span>
	</div>
	<div class="track__item-hit">hit</div>
</article>
