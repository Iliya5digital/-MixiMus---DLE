<article class="track__item flex flex__middle flex__between js-item" data-poster="[xfvalue_image_url_poster]" data-track="{THEME}/images/demo1.mp3" data-artist="Artist" data-title="{title}">
	<div class="flex flex__middle flex__first">
		[xfgiven_poster]<div class="track__item-poster"><figure><img src="[xfvalue_image_url_poster]" alt="{title}" width="55" height="55"/></figure></div>[/xfgiven_poster]
		<div ripple class="track__item-play js-ctrl js-play" title="Play/Stop">
			<span class="aicon aicon-play"></span>
		</div>
		<div class="track__item-metatitle">
			<div class="track__item-title"><a href="{link}" title="{title}">{title}</a></div>
			<div class="track__item-subtitle">Artist</div>
		</div>
	</div>
	<div class="track__item-time">02:37</div>
</article>
